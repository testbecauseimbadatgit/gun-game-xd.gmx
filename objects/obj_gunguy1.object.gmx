instance_create(x+6*image_xscale,y,obj_bullet_p1) 
instance_create(x+6*image_xscale,y,obj_bullet_p1) 
instance_create(x+6*image_xscale,y,obj_bullet_p1) 
instance_create(x+6*image_xscale,y,obj_bullet_p1) 
instance_create(x+6*image_xscale,y,obj_bullet_p1) 

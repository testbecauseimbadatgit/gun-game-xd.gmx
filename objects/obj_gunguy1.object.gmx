instance_create(x+6*image_xscale,y,obj_bullet_p1) 
instance_create(x+6*image_xscale,y,obj_bullet_p1) 
}
//Death
if hp &lt;= 0 then {
endtime += 1;
if endtime &gt;= 60 then room_restart();
}

            <string>///Input
right = keyboard_check_direct(ord("D"));
left = keyboard_check_direct(ord("A"));
jump = keyboard_check_direct(ord("W"));
shoot = keyboard_check_direct(vk_space);

//move left and right
if (left)
    {
        hsp = -4;
        image_xscale = -1;
        if (pistol == 0 &amp;&amp; shotgun == 0 &amp;&amp; lazer == 0) then sprite_index = gunguy1_walk
        if pistol == 1 then sprite_index = gunguy1_walk_pistol; 
        if lazer == 1 then sprite_index = gunguy1_walk_lazer;
        if shotgun == 1 then sprite_index = gunguy1_walk_shotgun;
    }
if (right)
    {
        hsp = 4;
        image_xscale = 1;
        if (pistol == 0 &amp;&amp; shotgun == 0 &amp;&amp; lazer == 0) then sprite_index = gunguy1_walk
        if pistol == 1 then sprite_index = gunguy1_walk_pistol;
        if lazer == 1 then sprite_index = gunguy1_walk_lazer;
        if shotgun == 1 then sprite_index = gunguy1_walk_shotgun;
    }

//neutral input = 0 movement
if ((right &amp;&amp; left) or (!right &amp;&amp; !left))
    {
        hsp = 0;
    }


//jumping
if (jump)
{
    if grounded = 1 then vsp = -8;     
}
    
//gravity
vsp += grav;

//vertical collision
if place_meeting (x,y+vsp,parent_blocks) then
{
    vsp = 0;
    grounded = 1;
}

else grounded = 0;

//horizontal collision   
if place_meeting(x+hsp,y-9,parent_blocks) then
{
     if (!place_meeting(x+sign(hsp),y,parent_blocks)) x+=sign(hsp);
     hsp = 0;
}

//Stairs
if place_meeting(x,y-2,parent_blocks) y -= 4;

//Ceiling collision
if place_meeting(x,y+vsp-4,parent_blocks) then vsp = 1;

//commit to movement
x += hsp;
y += vsp;

//idle
if (hsp = 0 &amp;&amp; vsp = 0) then 
{
    if (pistol == 0 &amp;&amp; lazer == 0 &amp;&amp; shotgun == 0) then sprite_index = gunguy1_idle;
    if pistol == 1 then sprite_index = gunguy1_idle_pistol;
    if lazer == 1 then sprite_index = gunguy1_idle_lazer;
    if shotgun == 1 then sprite_index = gunguy1_idle_shotgun;
}

//shooting
if pistol == 1 and shoot then
{
    instance_create(x+4*image_xscale,y,obj_bullet_p1)
}
if lazer == 1 and shoot then
{
    instance_create(x+6*image_xscale,y,obj_lazershot_p1)
}
if shotgun == 1 and shoot then
{
instance_create(x+6*image_xscale,y,obj_bullet_p1) 
instance_create(x+6*image_xscale,y,obj_bullet_p1) 
instance_create(x+6*image_xscale,y,obj_bullet_p1) 
